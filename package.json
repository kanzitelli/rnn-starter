{
  "name": "expo-rnn-starter",
  "version": "7.1.0",
  "description": "Expo SDK, React Native Navigation, Reanimated 2, Mobx, Dark Mode support, Expo Updates. Ready for production!",
  "license": "MIT",
  "author": "Batyr Kanzitdinov <say_hi@batyr.io>",
  "homepage": "https://github.com/kanzitelli/expo-rnn-starter",
  "scripts": {
    "android": "npx react-native run-android",
    "android:release": "yarn android --variant release",
    "ios": "npx react-native run-ios",
    "ios:release": "yarn ios --configuration Release",
    "ios:pods": "npx pod-install",
    "updates:publish:dev": "expo publish --release-channel dev",
    "u:p:dev": "yarn updates:publish:dev",
    "updates:optimize": "npx expo-optimize",
    "u:o": "yarn updates:optimize",
    "start": "npx react-native start",
    "test": "jest",
    "release": "dotenv npx release-it",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "expo": "^40.0.0",
    "expo-localization": "~9.1.0",
    "expo-network": "^2.4.0",
    "expo-updates": "^0.4.1",
    "hermes-engine": "^0.5.2-rc1",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.20",
    "mobx": "^6.0.4",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^7.0.5",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-device-info": "^7.3.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-navigation": "^7.7.0",
    "react-native-navigation-hooks": "^6.2.0",
    "react-native-notifications": "^3.4.2",
    "react-native-reanimated": "2.0.0-rc.0",
    "react-native-unimodules": "^0.12.0",
    "react-native-vector-icons": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/react-native": "^0.63.43",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^17.0.0",
    "babel-jest": "^26.6.3",
    "dotenv-cli": "^4.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "^17.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
